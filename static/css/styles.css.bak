        color: #474747 !important; }

.fb__nav-menu-right li {
  text-transform: uppercase;
  padding: 8px 30px;
  border-left: 1px solid #474747;
  color: #ffffff;
  font-size: 28px;
  line-height: 2; }
.fb__nav-menu-right li:first-child {
